
<div class="row">
  <div class="small-4 large-4 columns">
    <%= form_for :profile, url: profile_path, method: :patch do |f| %>
      <h4>Religion</h4>
      <%= f.text_field :religion %>
      <%= f.submit "Update", class: "button"%>
    <% end %>
    <%= link_to "cancel", users_dashboard_path%>
  </div>
</div>

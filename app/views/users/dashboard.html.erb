<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

  <div class="hide" id="side-bar">
    <span><%=link_to "X", "#", class: "on-top", id: "close-side" %><span>
    <div class="center-text" id="dash-controls">
      <ul>
        <% if current_user.profile %>
          <li> <%= link_to "Profile", 
            edit_profile_path(current_user.profile) %></li> 
        <% elsif %>
          <li><%= link_to "Create Profile", "#", 
            id: "new-profile", class: "on-top center-text" %></li>
        <% end %>

        <li><%= link_to "log out",destroy_user_session_path, 
          method: "delete", class: "on-top center-text" %></li>

      </ul>
    </div>
  </div>

  <div id = "nav" >
    <%= link_to "For Parents", "#", class: "button", id: "for-parents-link" %>
    <%= link_to "Dashboard", "#", class: "button right", id: "dashboard"%>
  </div>



  <div id="map-wrapper">
    <div id="map"></div>
  </div>

  <div class="row">
    <div class="small-12 large-12">
    </div>
  </div>

  <% unless current_user.profile%>
    <div id="modal-back">
    </div>
    <div class="show" id="modal">
      <div id="profile-wrapper">
        <div class="row">
          <div class="hide-for-small medium-12
            medium-centered large-8 large-centered columns">
              <%= image_tag "big-globe.png", id: "modal-globe" %>
          </div>
        </div>
        <div id="profileBox">
          <div class="row">
            <div class="small-8 small-centered medium-6
              medium-centered large-4 large-centered columns">
              <%= render partial: "/profiles/form" %>
              <%= link_to "I'll do this Later",
                "#",
                id: "close-modal",
                class: "button tiny secondary"%>
            </div>
          </div>
        </div>
      </div>
    </div>
  <%end%>
<script>
  <%= render "map"%>
</script>

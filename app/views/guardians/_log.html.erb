<div class = "right white lobster" id="interaction-count">
  <%= interactions.count%>
</div>
<h5 class="white">Below is a log of your kiddo's interactions</h4>
<div id = "interaction-list">
  <% interactions.each do |inter|%>
    <%= link_to raw("
      <div class='dash-knob white'>
        #{image_tag 'http://i.imgur.com/vebhdRE.png?1'}
        #{inter.user.email}
        | <span>#{inter.user.city}</span></div>
                    "), "#", id: "#{inter.id}" %>
    <div class="hide dash-knob-white", id = <%= "#{inter.id}-info" %>>
      <div id="conversation">
      </div>
      <ul>
        <li><span class = "blue-title">Guardian: </span>
        <%= inter.guardian.email%></li>
        <li><span class = "blue-title">Date:</span>
        <%= inter.created_time%></li> 
      </ul>
    </div>
  <% end %>
</div>

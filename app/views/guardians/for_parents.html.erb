<div class="dark-banner-auto">
  <div class="dark-banner">
  <%= link_to "Dash", users_dashboard_path, class: "button small nav-link", 
    id: "dashboard-link" %>

    <div class="row">
      <div class="large-4 columns">
        <h1 class="white lobster">For Parents</h1>
      </div>
      <div class="large-8 columns">
        <div class="hide" id="recent-nav">
          <%= link_to "day", guardians_recent_path(time: "day"), 
            remote: true,
            class: "button tiny", id: "time"%>
          <%= link_to "week", guardians_recent_path(time: "week"), 
            remote: true,
            class: "button small active", id: "time"%>
          <%= link_to "month", guardians_recent_path(time: "month"), 
            remote: true,
            class: "button tiny", id: "time"%>
          <%= link_to "back", guardians_for_parents_path, 
            class: "button tiny",
            id: "back"%>
        </div>
        <div id="nav">
          <%= link_to "Recent", 
            guardians_recent_path(time: "day"), 
            remote: true, 
            class: "button tiny", id: "recent"%>
        </div>
      </div>
    </div>
    <div class = "row">
      <div class = "large-12 columns">
        <div id = "log-partial">
          <%= render partial: "log", 
            locals: {interactions: @interactions}%>
        </div>
      </div>
    </div>
  </div>
</div>
